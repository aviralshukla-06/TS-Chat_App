generator client{
  provider = "prisma-client-js"
}

datasource db{
  provider = "postgresql"
  url = env("POSTGRESQL_URI")
}

model Users {
  id          Int         @id @default(autoincrement())
  email       String      @unique
  full_name    String      @map("fullName")
  password    String  
  profile_pic  String       @map("profilePic")
}
